                    <!--Order Pool Report-->
                    <TreeViewItem>
                        <TreeViewItem.Resources>
                            <Style TargetType="{x:Type TreeViewItem}">
                                <Style.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="IsExpanded" Value="{Binding IsOrderPoolReportEnabled}" />
                                    </Trigger>
                                    <DataTrigger Binding="{Binding Path=IsOrderPoolReportEnabled}" Value="False">
                                        <Setter Property="Foreground" Value="#cccccc"></Setter>
                                        <Setter Property="Cursor" Value="Arrow" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding Path=IsOrderPoolReportEnabled}" Value="True">
                                        <Setter Property="Foreground" Value="#000000"></Setter>
                                        <Setter Property="Cursor" Value="Hand" />
                                    </DataTrigger>
                                </Style.Triggers>
                                <Setter Property="HeaderTemplate">
                                    <Setter.Value>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal" Width="155" Height="25" Margin="-25,0,0,0">
                                                <TextBlock Text="ORDER POOL REPORT"  VerticalAlignment="Center" Width="155" Height="25" Padding="15,5,0,0" Margin="0 0"  
                                                           FontWeight="Bold" />
                                            </StackPanel>
                                        </DataTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </TreeViewItem.Resources>
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="PreviewMouseDown">
                                <i:InvokeCommandAction Command="{Binding MenuButtonPressCommand}" CommandParameter="OrderPoolReport" IsEnabled="{Binding IsOrderPoolReportEnabled}"  />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </TreeViewItem>
                    <!--Scheduling Report-->
                    <TreeViewItem>
                        <TreeViewItem.Resources>
                            <Style TargetType="{x:Type TreeViewItem}">
                                <Style.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="IsExpanded" Value="{Binding IsSchedulingReportEnabled}" />
                                    </Trigger>
                                    <DataTrigger Binding="{Binding Path=IsSchedulingReportEnabled}" Value="False">
                                        <Setter Property="Foreground" Value="#cccccc"></Setter>
                                        <Setter Property="Cursor" Value="Arrow" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding Path=IsSchedulingReportEnabled}" Value="True">
                                        <Setter Property="Foreground" Value="#000000"></Setter>
                                        <Setter Property="Cursor" Value="Hand" />
                                    </DataTrigger>
                                </Style.Triggers>
                                <Setter Property="HeaderTemplate">
                                    <Setter.Value>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal" Width="155" Height="25" Margin="-25,0,0,0">
                                                <TextBlock Text="SCHEDULING REPORT"  VerticalAlignment="Center" Width="155" Height="25" Padding="15,5,0,0" Margin="0 0"  
                                                           FontWeight="Bold" />
                                            </StackPanel>
                                        </DataTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </TreeViewItem.Resources>
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="PreviewMouseDown">
                                <i:InvokeCommandAction Command="{Binding MenuButtonPressCommand}" CommandParameter="SchedulingReport" IsEnabled="{Binding IsSchedulingReportEnabled}"  />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </TreeViewItem>
                </TreeViewItem>              
                
            </TreeView>
           
            <TextBlock Text="Send To Warehouse" TextAlignment="Center" FontWeight="Bold" Height="16" VerticalAlignment="Top" Grid.Row="1" Visibility="{Binding SendToWarehouseVisibility}" />
            <ListView ItemsSource="{Binding PendingSalesOrders}"  Grid.Row="1" Margin="1,16,0,0" 
                      ItemContainerStyle="{StaticResource alternatingSalesStyle}" 
                      AlternationCount="2" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Disabled" 
                      SelectedItem="{Binding DataContext.SelectedSalesOrder, RelativeSource={RelativeSource AncestorType=Window}}" Visibility="{Binding SendToWarehouseVisibility}" >
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <StackPanel>
                            <Grid>
                                <TextBlock FontSize="10" FontWeight="Bold" TextWrapping="Wrap" Width="200">
                                  <Run Text="SNO : "/>
                                  <Run Text="{Binding SalesOrderNo}"/>
                                </TextBlock>
                                <Button  Width="55" HorizontalAlignment="Right" FontSize="9" ToolTip="Send this order to warehouse for packing" 
                                         Command="{Binding DataContext.ProcessOrderCommand, RelativeSource={RelativeSource AncestorType={x:Type ListView}}}" 
                                         CommandParameter="{Binding}">PROCESS</Button>
                            </Grid>
                            <TextBlock FontSize="10" FontWeight="Bold" TextWrapping="Wrap" >
                              <Run Text="{Binding Customer.CompanyName}"/>
                            </TextBlock>

                            <ListView ItemsSource="{Binding SalesOrderDetails}" >
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn Width="90">
                                            <GridViewColumnHeader Content="Product" FontSize="9" FontWeight="Bold" />
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <TextBlock Text="{Binding Path=Product.ProductCode}" FontSize="10" TextWrapping="Wrap" />
                                                    </Grid>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>

                                        </GridViewColumn>
                                        <GridViewColumn >
                                            <GridViewColumnHeader Content="Qty" FontSize="9" FontWeight="Bold" />
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <TextBlock Text="{Binding Path=Quantity, StringFormat=0.##}" FontSize="10" TextAlignment="Right"  />
                                                    </Grid>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn >
                                            <GridViewColumnHeader Content="Res" FontSize="9" FontWeight="Bold" />
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <TextBlock Text="{Binding Path=QtyToMake, StringFormat=0.##}" FontSize="10" TextAlignment="Right"  />
                                                    </Grid>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn >
                                            <GridViewColumnHeader Content="Av Qty" FontSize="9" FontWeight="Bold" />
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <TextBlock Text="{Binding Path=QtyInStock, StringFormat=0.##}" FontSize="10" TextAlignment="Right"  />
                                                    </Grid>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                                <!--<ListView.ItemTemplate>
                                    <DataTemplate>
                                        <StackPanel>                                          
                                            <TextBlock Text="{Binding Product.ProductCode}"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </ListView.ItemTemplate>-->
                            </ListView>
                        </StackPanel>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>

            <TextBlock Text="Current Windows" TextAlignment="Center" FontWeight="Bold" Height="16" VerticalAlignment="Top" Grid.Row="2" Margin="0,2,0,0"  />
            <ListView  Margin="1,21,0,24" ItemsSource="{Binding Items,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" SelectedItem="{Binding SelectedTitle,Mode=OneWayToSource}" DisplayMemberPath="Title" SelectionMode="Single" Grid.Row="2" ItemContainerStyle="{DynamicResource _ListBoxItemStyle}" AlternationCount="2" >
                <ListView.ContextMenu>
                    <ContextMenu>
                        <MenuItem Header="Close Window" 
                                  Command="{Binding DataContext.RemoveItemCommand, RelativeSource={RelativeSource AncestorType={x:Type ContextMenu}}}" 
                                  CommandParameter="{Binding SelectedTitle}"  />
                    </ContextMenu>
                </ListView.ContextMenu>
            </ListView>
            
        </Grid>
        <!--</StackPanel>-->


        <MDI:MDIContainer Name="MDI" Grid.Column="1" Margin="4,4,4,6" ItemsSource="{Binding Items}" SelectedItem="{Binding SelectedWindow, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True}" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" >
            <MDI:MDIContainer.Resources>
                <Style TargetType="{x:Type MDI:MDIWindow}">
                    <Setter Property="Title" Value="{Binding Title}" />
                    <Setter Property="CanClose" Value="{Binding CanClose, UpdateSourceTrigger=PropertyChanged}" />
                    <Setter Property="WindowState" Value="Maximized" />

                    <!--<Setter Property="Width" Value="Auto" />-->

                </Style>
            </MDI:MDIContainer.Resources>
        </MDI:MDIContainer>
        <TextBlock Panel.ZIndex="99999" Foreground="Black" FontWeight="ExtraBlack" FontSize="10" Text="{Binding Version}" TextAlignment="Center" Padding="0,0,5,0"  Margin="56,0,0,5" Height="15" VerticalAlignment="Bottom" />
        <Button Content="LOG OUT" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="56" Height="20" HorizontalContentAlignment="Left" Margin="0,0,0,3" Command="{Binding LogOutCommand}" ToolTip="Log out from system" TabIndex="2" >
            <Button.Style>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="Background" Value="#666666"/>
                    <Setter Property="Foreground" Value="White"/>
                    <Setter Property="FontWeight" Value="Bold"/>
                    <Setter Property="FontSize" Value="10"/>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Button}">
                                <Border Background="{TemplateBinding Background}" BorderThickness="2,2,2,2" CornerRadius="4,4,4,4">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </Border>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                    <Style.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="#4c4c4c"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </Button.Style>
        </Button>
        <Border x:Name="childWindow" Visibility="{Binding WindowVisibility}" Background="#88000000" RenderTransformOrigin="0.5,0.5" Panel.ZIndex="99999" Grid.ColumnSpan="2">
            <ContentControl Content="{Binding XmlContent}"  HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"/>
        </Border>
    </Grid>
</Window>
